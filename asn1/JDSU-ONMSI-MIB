jdsuOnmsiAlarmSplitterPeakTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiAlarmSplitterPeakEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains peak details for a splitter fault. Load it with jdsuOnmsiAlarmSplitterPeakLoad"
	::= { jdsuOnmsiAlarmData 4 }


jdsuOnmsiAlarmSplitterPeakEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiAlarmSplitterPeakEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION ""
	INDEX {
		jdsuOnmsiAlarmSplitterPeakEntryInternalKey }
	::= { jdsuOnmsiAlarmSplitterPeakTable 1 }


JdsuOnmsiAlarmSplitterPeakEntry ::= SEQUENCE {

	jdsuOnmsiAlarmSplitterPeakEntryInternalKey             JdsuOnmsiInternalKey,
	jdsuOnmsiAlarmSplitterPeakEntryName                    JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmSplitterPeakEntryPerceivedSeverity       JdsuOnmsiSeverity,
	jdsuOnmsiAlarmSplitterPeakEntryPreviousSeverity        JdsuOnmsiSeverity,
	jdsuOnmsiAlarmSplitterPeakEntryReferenceBottomDistance INTEGER,
	jdsuOnmsiAlarmSplitterPeakEntryReferenceTopDistance    INTEGER,
	jdsuOnmsiAlarmSplitterPeakEntryReferenceLevel          INTEGER,
	jdsuOnmsiAlarmSplitterPeakEntryPeakMeasurementValid    TruthValue,
	jdsuOnmsiAlarmSplitterPeakEntryMeasuredBottomDistance  INTEGER,
	jdsuOnmsiAlarmSplitterPeakEntryMeasuredTopDistance     INTEGER,
	jdsuOnmsiAlarmSplitterPeakEntryMeasuredLevel           INTEGER }


jdsuOnmsiAlarmSplitterPeakEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The peak internal key"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 1 }


jdsuOnmsiAlarmSplitterPeakEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The peak name"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 2 }


jdsuOnmsiAlarmSplitterPeakEntryPerceivedSeverity OBJECT-TYPE
	SYNTAX  JdsuOnmsiSeverity
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The severity of the peak"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 3 }


jdsuOnmsiAlarmSplitterPeakEntryPreviousSeverity OBJECT-TYPE
	SYNTAX  JdsuOnmsiSeverity
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The previous severity of the peak"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 4 }


jdsuOnmsiAlarmSplitterPeakEntryReferenceBottomDistance OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The distance of the reference peak's bottom
		Unit : millimeters"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 5 }


jdsuOnmsiAlarmSplitterPeakEntryReferenceTopDistance OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The distance of the reference peak's top
		Unit : millimeters"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 6 }


jdsuOnmsiAlarmSplitterPeakEntryReferenceLevel OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The level of the reference peak
		Unit : dB / 100"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 7 }


jdsuOnmsiAlarmSplitterPeakEntryPeakMeasurementValid OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"Whether the peak measurement is valid"
	::= { jdsuOnmsiAlarmSplitterPeakEntry 8 }


jdsuOnmsiAlarmSplitterPeakEntryMeasuredBottomDistance OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
