		"This table contains the additional attributes on central offices"
	::= { jdsuOnmsiCentralOfficeData 2 }


jdsuOnmsiCentralOfficeAdditionalAttributeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiCentralOfficeAdditionalAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A central office additional attribute entry"
	INDEX {
		jdsuOnmsiCentralOfficeAdditionalAttributeEntryInternalKey,
		jdsuOnmsiCentralOfficeAdditionalAttributeEntryName }
	::= { jdsuOnmsiCentralOfficeAdditionalAttributeTable 1 }


JdsuOnmsiCentralOfficeAdditionalAttributeEntry ::= SEQUENCE {

	jdsuOnmsiCentralOfficeAdditionalAttributeEntryInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiCentralOfficeAdditionalAttributeEntryName        JdsuOnmsiUtf8String,
	jdsuOnmsiCentralOfficeAdditionalAttributeEntryValue       JdsuOnmsiUtf8String }


jdsuOnmsiCentralOfficeAdditionalAttributeEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the central office"
	::= { jdsuOnmsiCentralOfficeAdditionalAttributeEntry 1 }


jdsuOnmsiCentralOfficeAdditionalAttributeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute name"
	::= { jdsuOnmsiCentralOfficeAdditionalAttributeEntry 2 }


jdsuOnmsiCentralOfficeAdditionalAttributeEntryValue OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute value"
	::= { jdsuOnmsiCentralOfficeAdditionalAttributeEntry 3 }


jdsuOnmsiCentralOfficeAttributeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiCentralOfficeAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the descriptions of noticeable central office attributes"
	::= { jdsuOnmsiCentralOfficeData 3 }


jdsuOnmsiCentralOfficeAttributeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiCentralOfficeAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A central office attribute entry"
	INDEX {
		jdsuOnmsiCentralOfficeAttributeEntryName }
	::= { jdsuOnmsiCentralOfficeAttributeTable 1 }


JdsuOnmsiCentralOfficeAttributeEntry ::= SEQUENCE {

	jdsuOnmsiCentralOfficeAttributeEntryName       JdsuOnmsiUtf8String,
	jdsuOnmsiCentralOfficeAttributeEntryAdditional TruthValue,
	jdsuOnmsiCentralOfficeAttributeEntryFindable   TruthValue,
	jdsuOnmsiCentralOfficeAttributeEntryUpdatable  TruthValue }


jdsuOnmsiCentralOfficeAttributeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The attribute name. The additional attributes are prefixed with an underscore character to avoid name collision with standard attributes"
	::= { jdsuOnmsiCentralOfficeAttributeEntry 1 }


jdsuOnmsiCentralOfficeAttributeEntryAdditional OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is additional"
	::= { jdsuOnmsiCentralOfficeAttributeEntry 2 }


jdsuOnmsiCentralOfficeAttributeEntryFindable OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is findable"
	::= { jdsuOnmsiCentralOfficeAttributeEntry 3 }


jdsuOnmsiCentralOfficeAttributeEntryUpdatable OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is updatable"
	::= { jdsuOnmsiCentralOfficeAttributeEntry 4 }


