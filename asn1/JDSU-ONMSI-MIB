	::= { jdsuOnmsiAlarmSplitterPeakLoad 2 }


jdsuOnmsiAlarmSplitterPeakLoadParamEventInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The internal key of the event for which the splitter peak should be loaded"
	::= { jdsuOnmsiAlarmSplitterPeakLoad 1 }


jdsuOnmsiAlarmSplitterPeakLoadError OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"When the function execution fails, this node contains information on the error that occurred.
		This node is empty when the execution is successful."
	::= { jdsuOnmsiAlarmSplitterPeakLoad 3 }


-------------------------------------------------------------------------------
-- Definition of the OTU service
-------------------------------------------------------------------------------

jdsuOnmsiOtuService OBJECT IDENTIFIER 	::= { jdsuOnmsiServices 7 }

-------------------------------------------------------------------------------
-- Data of the OTU service
-------------------------------------------------------------------------------

jdsuOnmsiOtuData OBJECT IDENTIFIER 	::= { jdsuOnmsiOtuService 1 }

jdsuOnmsiOtuTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiOtuEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the standard OTU data"
	::= { jdsuOnmsiOtuData 1 }


jdsuOnmsiOtuEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiOtuEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"An OTU entry"
	INDEX {
		jdsuOnmsiOtuEntryInternalKey }
	::= { jdsuOnmsiOtuTable 1 }


JdsuOnmsiOtuEntry ::= SEQUENCE {

	jdsuOnmsiOtuEntryInternalKey              JdsuOnmsiInternalKey,
	jdsuOnmsiOtuEntryName                     JdsuOnmsiUtf8String,
	jdsuOnmsiOtuEntryDescription              JdsuOnmsiUtf8String,
	jdsuOnmsiOtuEntryIpAddress                JdsuOnmsiUtf8String,
	jdsuOnmsiOtuEntryDeviceName               DisplayString,
	jdsuOnmsiOtuEntrySerialNumber             DisplayString,
	jdsuOnmsiOtuEntrySoftwareVersion          DisplayString,
	jdsuOnmsiOtuEntryCentralOfficeInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiOtuEntrySeverity                 JdsuOnmsiSeverity }


jdsuOnmsiOtuEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the OTU"
	::= { jdsuOnmsiOtuEntry 1 }


jdsuOnmsiOtuEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The name of the OTU"
	::= { jdsuOnmsiOtuEntry 2 }


jdsuOnmsiOtuEntryDescription OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The description of the OTU"
	::= { jdsuOnmsiOtuEntry 3 }


jdsuOnmsiOtuEntryIpAddress OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The IP address of hostname of the OTU"
	::= { jdsuOnmsiOtuEntry 4 }


jdsuOnmsiOtuEntryDeviceName OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The device name of the OTU ('OTU 8000', 'OTU 8000E', ...)"
	::= { jdsuOnmsiOtuEntry 5 }


jdsuOnmsiOtuEntrySerialNumber OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The serial number of the OTU"
	::= { jdsuOnmsiOtuEntry 6 }


jdsuOnmsiOtuEntrySoftwareVersion OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The software version of the OTU"
	::= { jdsuOnmsiOtuEntry 7 }


jdsuOnmsiOtuEntryCentralOfficeInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the central office associated to the OTU"
	::= { jdsuOnmsiOtuEntry 8 }


jdsuOnmsiOtuEntrySeverity OBJECT-TYPE
	SYNTAX  JdsuOnmsiSeverity
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The current severity of the OTU"
	::= { jdsuOnmsiOtuEntry 9 }


jdsuOnmsiOtuAdditionalAttributeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiOtuAdditionalAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the additional attributes on OTUs"
	::= { jdsuOnmsiOtuData 2 }


jdsuOnmsiOtuAdditionalAttributeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiOtuAdditionalAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"An OTU additional attribute entry"
	INDEX {
		jdsuOnmsiOtuAdditionalAttributeEntryInternalKey,
		jdsuOnmsiOtuAdditionalAttributeEntryName }
	::= { jdsuOnmsiOtuAdditionalAttributeTable 1 }


JdsuOnmsiOtuAdditionalAttributeEntry ::= SEQUENCE {

	jdsuOnmsiOtuAdditionalAttributeEntryInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiOtuAdditionalAttributeEntryName        JdsuOnmsiUtf8String,
	jdsuOnmsiOtuAdditionalAttributeEntryValue       JdsuOnmsiUtf8String }


jdsuOnmsiOtuAdditionalAttributeEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the OTU"
	::= { jdsuOnmsiOtuAdditionalAttributeEntry 1 }


jdsuOnmsiOtuAdditionalAttributeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute name"
	::= { jdsuOnmsiOtuAdditionalAttributeEntry 2 }


jdsuOnmsiOtuAdditionalAttributeEntryValue OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute value"
	::= { jdsuOnmsiOtuAdditionalAttributeEntry 3 }


jdsuOnmsiOtuAttributeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiOtuAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the descriptions of noticeable OTU attributes"
	::= { jdsuOnmsiOtuData 3 }


jdsuOnmsiOtuAttributeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiOtuAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"An OTU attribute entry"
	INDEX {
		jdsuOnmsiOtuAttributeEntryName }
	::= { jdsuOnmsiOtuAttributeTable 1 }


JdsuOnmsiOtuAttributeEntry ::= SEQUENCE {

	jdsuOnmsiOtuAttributeEntryName       JdsuOnmsiUtf8String,
	jdsuOnmsiOtuAttributeEntryAdditional TruthValue,
	jdsuOnmsiOtuAttributeEntryFindable   TruthValue,
	jdsuOnmsiOtuAttributeEntryUpdatable  TruthValue }


jdsuOnmsiOtuAttributeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The attribute name. The additional attributes are prefixed with an underscore character to avoid name collision with standard attributes"
	::= { jdsuOnmsiOtuAttributeEntry 1 }


jdsuOnmsiOtuAttributeEntryAdditional OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is additional"
	::= { jdsuOnmsiOtuAttributeEntry 2 }


jdsuOnmsiOtuAttributeEntryFindable OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is findable"
	::= { jdsuOnmsiOtuAttributeEntry 3 }


jdsuOnmsiOtuAttributeEntryUpdatable OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is updatable"
	::= { jdsuOnmsiOtuAttributeEntry 4 }


jdsuOnmsiOtuOtdrTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiOtuOtdrEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the data on the OTDR modules of OTUs"
	::= { jdsuOnmsiOtuData 4 }


jdsuOnmsiOtuOtdrEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiOtuOtdrEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"An OTDR module entry"
	INDEX {
		jdsuOnmsiOtuOtdrEntryInternalKey }
	::= { jdsuOnmsiOtuOtdrTable 1 }


JdsuOnmsiOtuOtdrEntry ::= SEQUENCE {

	jdsuOnmsiOtuOtdrEntryInternalKey    JdsuOnmsiInternalKey,
	jdsuOnmsiOtuOtdrEntryOtuInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiOtuOtdrEntryPosition       JdsuOnmsiOtdrPosition,
	jdsuOnmsiOtuOtdrEntryType           DisplayString,
	jdsuOnmsiOtuOtdrEntrySerialNumber   DisplayString,
	jdsuOnmsiOtuOtdrEntryWavelengths    DisplayString }


jdsuOnmsiOtuOtdrEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the OTDR module"
	::= { jdsuOnmsiOtuOtdrEntry 1 }


jdsuOnmsiOtuOtdrEntryOtuInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the OTU to which the OTDR module belongs"
	::= { jdsuOnmsiOtuOtdrEntry 2 }


jdsuOnmsiOtuOtdrEntryPosition OBJECT-TYPE
	SYNTAX  JdsuOnmsiOtdrPosition
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The position of the OTDR module in the OTU, as a JdsuOnmsiOtdrPosition:
		1: top
		2: bottom"
	::= { jdsuOnmsiOtuOtdrEntry 3 }


jdsuOnmsiOtuOtdrEntryType OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The type of OTDR module (ex: '8117R VLR')"
	::= { jdsuOnmsiOtuOtdrEntry 4 }


jdsuOnmsiOtuOtdrEntrySerialNumber OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The serial number of the OTDR module"
	::= { jdsuOnmsiOtuOtdrEntry 5 }


jdsuOnmsiOtuOtdrEntryWavelengths OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The list of wavelengths supported by the OTDR module (ex: '1550, 1625')"
	::= { jdsuOnmsiOtuOtdrEntry 6 }


jdsuOnmsiOtuSwitchTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiOtuSwitchEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the data on the switches of OTUs"
	::= { jdsuOnmsiOtuData 5 }


jdsuOnmsiOtuSwitchEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiOtuSwitchEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A switch entry"
	INDEX {
		jdsuOnmsiOtuSwitchEntryInternalKey }
	::= { jdsuOnmsiOtuSwitchTable 1 }


JdsuOnmsiOtuSwitchEntry ::= SEQUENCE {

	jdsuOnmsiOtuSwitchEntryInternalKey    JdsuOnmsiInternalKey,
	jdsuOnmsiOtuSwitchEntryOtuInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiOtuSwitchEntryName           JdsuOnmsiUtf8String,
	jdsuOnmsiOtuSwitchEntryRemote         TruthValue,
	jdsuOnmsiOtuSwitchEntryPosition       INTEGER,
	jdsuOnmsiOtuSwitchEntryInputs         INTEGER,
	jdsuOnmsiOtuSwitchEntryOutputs        INTEGER }


jdsuOnmsiOtuSwitchEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the switch"
	::= { jdsuOnmsiOtuSwitchEntry 1 }


jdsuOnmsiOtuSwitchEntryOtuInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the OTU to which the switch belongs"
	::= { jdsuOnmsiOtuSwitchEntry 2 }


jdsuOnmsiOtuSwitchEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The name of the switch"
	::= { jdsuOnmsiOtuSwitchEntry 3 }


jdsuOnmsiOtuSwitchEntryRemote OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the switch is remote"
	::= { jdsuOnmsiOtuSwitchEntry 4 }


jdsuOnmsiOtuSwitchEntryPosition OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The position of the switch in the OTU"
	::= { jdsuOnmsiOtuSwitchEntry 5 }


jdsuOnmsiOtuSwitchEntryInputs OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The switch input count"
	::= { jdsuOnmsiOtuSwitchEntry 6 }


jdsuOnmsiOtuSwitchEntryOutputs OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The switch output count"
	::= { jdsuOnmsiOtuSwitchEntry 7 }


jdsuOnmsiOtuPortTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiOtuPortEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the data on the ports of OTUs"
	::= { jdsuOnmsiOtuData 6 }


jdsuOnmsiOtuPortEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiOtuPortEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A port entry"
	INDEX {
		jdsuOnmsiOtuPortEntryInternalKey }
	::= { jdsuOnmsiOtuPortTable 1 }


JdsuOnmsiOtuPortEntry ::= SEQUENCE {

	jdsuOnmsiOtuPortEntryInternalKey       JdsuOnmsiInternalKey,
	jdsuOnmsiOtuPortEntryOtuInternalKey    JdsuOnmsiInternalKey,
	jdsuOnmsiOtuPortEntryModuleInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiOtuPortEntryPosition          INTEGER,
	jdsuOnmsiOtuPortEntryPonInternalKey    JdsuOnmsiInternalKey,
	jdsuOnmsiOtuPortEntryLinkInternalKey   JdsuOnmsiInternalKey }


jdsuOnmsiOtuPortEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the OTU to which the port belongs"
	::= { jdsuOnmsiOtuPortEntry 1 }


jdsuOnmsiOtuPortEntryOtuInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the OTU to which the port belongs"
	::= { jdsuOnmsiOtuPortEntry 2 }


jdsuOnmsiOtuPortEntryModuleInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the module (which may be a jdsuOnmsiOtuSwitchEntryInternalKey or a jdsuOnmsiOtuOtdrEntryInternalKey) to which the port belongs."
	::= { jdsuOnmsiOtuPortEntry 3 }


jdsuOnmsiOtuPortEntryPosition OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The output number of the port"
	::= { jdsuOnmsiOtuPortEntry 4 }


jdsuOnmsiOtuPortEntryPonInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the PON associated to the port"
	::= { jdsuOnmsiOtuPortEntry 5 }


jdsuOnmsiOtuPortEntryLinkInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the link associated to the port"
	::= { jdsuOnmsiOtuPortEntry 6 }


-------------------------------------------------------------------------------
-- Functions of the OTU service
-------------------------------------------------------------------------------

jdsuOnmsiOtuFunctions OBJECT IDENTIFIER 	::= { jdsuOnmsiOtuService 2 }

-------------------------------------------------------------------------------
-- Function to get an OTU
-- Results will fill jdsuOnmsiOtuTable, jdsuOnmsiOtuOtdrTable, jdsuOnmsiOtuSwitchTable and jdsuOnmsiOtuAdditionalAttributeTable
-------------------------------------------------------------------------------

jdsuOnmsiOtuGet OBJECT IDENTIFIER 	::= { jdsuOnmsiOtuFunctions 1 }

jdsuOnmsiOtuGetParamInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The internal key of the OTU to retrieve"
	::= { jdsuOnmsiOtuGet 1 }


jdsuOnmsiOtuGetExecute OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Setting this field to 1 performs the OTU retrieval"
	::= { jdsuOnmsiOtuGet 2 }


jdsuOnmsiOtuGetError OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"When the function execution fails, this node contains information on the error that occurred.
		This node is empty when the execution is successful."
	::= { jdsuOnmsiOtuGet 3 }


-------------------------------------------------------------------------------
-- Function to find OTUs
-- Results will fill jdsuOnmsiOtuTable, jdsuOnmsiOtuOtdrTable, jdsuOnmsiOtuSwitchTable and jdsuOnmsiOtuAdditionalAttributeTable
-------------------------------------------------------------------------------

jdsuOnmsiOtuFind OBJECT IDENTIFIER 	::= { jdsuOnmsiOtuFunctions 2 }

jdsuOnmsiOtuFindParamAttribute OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The name of the OTU attribute to perform the search on.
		Valid attributes are :
		- name
		- ipAddress
		- serialNumber
		- centralOfficeInternalKey
		and any additional attribute name"
	::= { jdsuOnmsiOtuFind 1 }


jdsuOnmsiOtuFindParamValue OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The attribute value to find"
	::= { jdsuOnmsiOtuFind 2 }


jdsuOnmsiOtuFindExecute OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Setting this field to 1 performs the OTU find"
	::= { jdsuOnmsiOtuFind 3 }


jdsuOnmsiOtuFindError OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"When the function execution fails, this node contains information on the error that occurred.
		This node is empty when the execution is successful."
	::= { jdsuOnmsiOtuFind 4 }


-------------------------------------------------------------------------------
-- Function to load the OTU attributes
-- Results will fill the jdsuOnmsiOtuAttributeTable
-------------------------------------------------------------------------------

jdsuOnmsiOtuAttributeLoad OBJECT IDENTIFIER 	::= { jdsuOnmsiOtuFunctions 3 }

jdsuOnmsiOtuAttributeLoadExecute OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Setting this field to 1 performs the jdsuOnmsiOtuAttributeTable load"
	::= { jdsuOnmsiOtuAttributeLoad 1 }


jdsuOnmsiOtuAttributeLoadError OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"When the function execution fails, this node contains information on the error that occurred.
		This node is empty when the execution is successful."
	::= { jdsuOnmsiOtuAttributeLoad 2 }


-------------------------------------------------------------------------------
-- Function to get the ports for an OTU
-- Results will fill jdsuOnmsiOtuPortsTable
-------------------------------------------------------------------------------

jdsuOnmsiOtuGetPorts OBJECT IDENTIFIER 	::= { jdsuOnmsiOtuFunctions 4 }

