	jdsuOnmsiAlarmEventEntryEventMonitoringTestSpecificInfos       JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventPointToPointSpecificInfos         JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventAttenuationSpecificInfos          JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventPeakSpecificInfos                 JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventOrlSpecificInfos                  JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventFiberLengthExtensionSpecificInfos JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventLocalizationSpecificInfos         JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmInternalKey                       JdsuOnmsiInternalKey,
	jdsuOnmsiAlarmEventEntryAlarmType                              IANAItuEventType,
	jdsuOnmsiAlarmEventEntryAlarmRaisedTime                        DateAndTime,
	jdsuOnmsiAlarmEventEntryAlarmChangedTime                       DateAndTime,
	jdsuOnmsiAlarmEventEntryAlarmClearStatus                       JdsuOnmsiAlarmClearStatus,
	jdsuOnmsiAlarmEventEntryAlarmAckStatus                         JdsuOnmsiAlarmAckStatus,
	jdsuOnmsiAlarmEventEntryAlarmPerceivedSeverity                 JdsuOnmsiSeverity,
	jdsuOnmsiAlarmEventEntryAlarmSpecificProblem                   DisplayString,
	jdsuOnmsiAlarmEventEntryAlarmOriginatingEntityKind             DisplayString,
	jdsuOnmsiAlarmEventEntryAlarmOriginatingEntityInternalKey      JdsuOnmsiInternalKey,
	jdsuOnmsiAlarmEventEntryAlarmOriginatingEntityName             JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmOriginatingEntityOid              JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmSystemDN                          DisplayString,
	jdsuOnmsiAlarmEventEntryAlarmProbableCause                     IANAItuProbableCause,
	jdsuOnmsiAlarmEventEntryAlarmAdditionalText                    JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmMonitoringTestSpecificInfos       JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmPointToPointSpecificInfos         JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmAttenuationSpecificInfos          JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmPeakSpecificInfos                 JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmOrlSpecificInfos                  JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmFiberLengthExtensionSpecificInfos JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmLocalizationSpecificInfos         JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmOriginatingEntity                 JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventGpsCoordinatesSpecificInfos       JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmGpsCoordinatesSpecificInfos       JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventPonSpecificInfos                  JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmPonSpecificInfos                  JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventSplitterSpecificInfos             JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmSplitterSpecificInfos             JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventStrainSpecificInfos               JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmStrainSpecificInfos               JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryEventTemperatureSpecificInfos          JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmTemperatureSpecificInfos          JdsuOnmsiUtf8String,
	jdsuOnmsiAlarmEventEntryAlarmAdditionalAttributes              JdsuOnmsiUtf8String }


jdsuOnmsiAlarmEventEntrySequence OBJECT-TYPE
	SYNTAX  Unsigned32
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This is the sequence of the alarm event.
		It is used for synchronization between ONMSi and the SNMP manager.
		This sequence number is unique (for each alarm event/manager community/user) and
		there is no gap between two alarm events, to ensure no alarm event is ever missed.
		A resynchronization will reset the sequence to 1"
	::= { jdsuOnmsiAlarmEventEntry 1 }


jdsuOnmsiAlarmEventEntryEventInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the alarm event"
	::= { jdsuOnmsiAlarmEventEntry 2 }


