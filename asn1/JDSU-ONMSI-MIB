-------------------------------------------------------------------------------
-- Definition of the monitoring test service
-------------------------------------------------------------------------------

jdsuOnmsiMonitoringTestService OBJECT IDENTIFIER 	::= { jdsuOnmsiServices 5 }

-------------------------------------------------------------------------------
-- Data of the monitoring test service
-------------------------------------------------------------------------------

jdsuOnmsiMonitoringTestData OBJECT IDENTIFIER 	::= { jdsuOnmsiMonitoringTestService 1 }

jdsuOnmsiMonitoringTestTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiMonitoringTestEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the standard link data"
	::= { jdsuOnmsiMonitoringTestData 1 }


jdsuOnmsiMonitoringTestEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiMonitoringTestEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A monitoring test entry"
	INDEX {
		jdsuOnmsiMonitoringTestEntryInternalKey }
	::= { jdsuOnmsiMonitoringTestTable 1 }


JdsuOnmsiMonitoringTestEntry ::= SEQUENCE {

	jdsuOnmsiMonitoringTestEntryInternalKey               JdsuOnmsiInternalKey,
	jdsuOnmsiMonitoringTestEntryName                      JdsuOnmsiUtf8String,
	jdsuOnmsiMonitoringTestEntryDescription               JdsuOnmsiUtf8String,
	jdsuOnmsiMonitoringTestEntryLinkInternalKey           JdsuOnmsiInternalKey,
	jdsuOnmsiMonitoringTestEntryDisplayOrder              INTEGER,
	jdsuOnmsiMonitoringTestEntrySeverity                  JdsuOnmsiSeverity,
	jdsuOnmsiMonitoringTestEntryDetectionConfiguration    JdsuOnmsiUtf8String,
	jdsuOnmsiMonitoringTestEntryLocalizationConfiguration JdsuOnmsiUtf8String,
	jdsuOnmsiMonitoringTestEntrySchedulingConfiguration   JdsuOnmsiUtf8String }


jdsuOnmsiMonitoringTestEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the monitoring test"
	::= { jdsuOnmsiMonitoringTestEntry 1 }


jdsuOnmsiMonitoringTestEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The name of the monitoring test"
	::= { jdsuOnmsiMonitoringTestEntry 2 }


jdsuOnmsiMonitoringTestEntryDescription OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The description of the monitoring test"
	::= { jdsuOnmsiMonitoringTestEntry 3 }


jdsuOnmsiMonitoringTestEntryLinkInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the monitoring test's link"
	::= { jdsuOnmsiMonitoringTestEntry 4 }


jdsuOnmsiMonitoringTestEntryDisplayOrder OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The display order ot the monitoring test"
	::= { jdsuOnmsiMonitoringTestEntry 5 }


jdsuOnmsiMonitoringTestEntrySeverity OBJECT-TYPE
	SYNTAX  JdsuOnmsiSeverity
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The severity of the monitoring test"
	::= { jdsuOnmsiMonitoringTestEntry 6 }


jdsuOnmsiMonitoringTestEntryDetectionConfiguration OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The detection configuration of the monitoring test in a serialized form (key1=value1|key2=value2|...).
		It contains the following keys:
		- firstMarkerDistanceM: the distance of the first marker, in meters
		- lastMarkerDistanceM: the distance of the last marker, in meters
		- acqDurationSec: the acquisition duration of the detection reference, in seconds
		- pulseNs: the pulse of the detection reference, in ns
		- rangeKm: the range of the detection reference, in km
		- fiberIndex: the fiber index of the detection reference
		- resolutionCm: the resolution of the detection reference, in cm
		- wavelengthNm: the wavelength of the detection reference, in nm"
	::= { jdsuOnmsiMonitoringTestEntry 7 }


jdsuOnmsiMonitoringTestEntryLocalizationConfiguration OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The localization configuration of the monitoring test in a serialized form (key1=value1|key2=value2|...).
		It contains the following keys:
		- acqDurationSec: the acquisition duration of the localization reference, in seconds
		- pulseNs: the pulse of the localization reference, in ns
		- rangeKm: the range of the localization reference, in km
		- fiberIndex: the fiber index of the localization reference
		- resolutionCm: the resolution of the localization reference, in cm
		- wavelengthNm: the wavelength of the localization reference, in nm"
	::= { jdsuOnmsiMonitoringTestEntry 8 }


jdsuOnmsiMonitoringTestEntrySchedulingConfiguration OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The scheduling configuration of the monitoring test in a serialized form (key1=value1|key2=value2|...).
		It contains the following keys:
		- scheduled: this flag indicates whether the monitoring test is periodic or not (true/false)
		- periodical: this flag indicates whether the monitoring test is scheduled or not (true/false)
		- startDate: the date at which the monitoring test scheduling starts, in seconds since January 1, 1970, 00:00:00 GMT
		- frequencyMin: the period of the monitoring test scheduling (if the test is periodic), in minutes"
	::= { jdsuOnmsiMonitoringTestEntry 9 }


jdsuOnmsiMonitoringTestAdditionalAttributeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiMonitoringTestAdditionalAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the additional attributes on monitoring tests"
	::= { jdsuOnmsiMonitoringTestData 2 }


jdsuOnmsiMonitoringTestAdditionalAttributeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiMonitoringTestAdditionalAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A monitoring test additional attribute entry"
	INDEX {
		jdsuOnmsiMonitoringTestAdditionalAttributeEntryInternalKey,
		jdsuOnmsiMonitoringTestAdditionalAttributeEntryName }
	::= { jdsuOnmsiMonitoringTestAdditionalAttributeTable 1 }


JdsuOnmsiMonitoringTestAdditionalAttributeEntry ::= SEQUENCE {

	jdsuOnmsiMonitoringTestAdditionalAttributeEntryInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiMonitoringTestAdditionalAttributeEntryName        JdsuOnmsiUtf8String,
	jdsuOnmsiMonitoringTestAdditionalAttributeEntryValue       JdsuOnmsiUtf8String }


jdsuOnmsiMonitoringTestAdditionalAttributeEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the monitoring test"
	::= { jdsuOnmsiMonitoringTestAdditionalAttributeEntry 1 }


jdsuOnmsiMonitoringTestAdditionalAttributeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute name"
	::= { jdsuOnmsiMonitoringTestAdditionalAttributeEntry 2 }


jdsuOnmsiMonitoringTestAdditionalAttributeEntryValue OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute value"
	::= { jdsuOnmsiMonitoringTestAdditionalAttributeEntry 3 }


