-------------------------------------------------------------------------------
-- Definition of the PON service
-------------------------------------------------------------------------------

jdsuOnmsiPonService OBJECT IDENTIFIER 	::= { jdsuOnmsiServices 2 }

-------------------------------------------------------------------------------
-- Data of the PON service
-------------------------------------------------------------------------------

jdsuOnmsiPonData OBJECT IDENTIFIER 	::= { jdsuOnmsiPonService 1 }

jdsuOnmsiPonTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiPonEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the standard PON data"
	::= { jdsuOnmsiPonData 1 }


jdsuOnmsiPonEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiPonEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A PON entry"
	INDEX {
		jdsuOnmsiPonEntryInternalKey }
	::= { jdsuOnmsiPonTable 1 }


JdsuOnmsiPonEntry ::= SEQUENCE {

	jdsuOnmsiPonEntryInternalKey             JdsuOnmsiInternalKey,
	jdsuOnmsiPonEntryName                    JdsuOnmsiUtf8String,
	jdsuOnmsiPonEntryDescription             JdsuOnmsiUtf8String,
	jdsuOnmsiPonEntryLatestTestInternalKey   JdsuOnmsiInternalKey,
	jdsuOnmsiPonEntryOtuInternalKey          JdsuOnmsiInternalKey,
	jdsuOnmsiPonEntryPortInternalKey         JdsuOnmsiInternalKey,
	jdsuOnmsiPonEntrySeverity                JdsuOnmsiSeverity,
	jdsuOnmsiPonEntrySchedulingConfiguration JdsuOnmsiUtf8String }


jdsuOnmsiPonEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the PON"
	::= { jdsuOnmsiPonEntry 1 }


jdsuOnmsiPonEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The name of the PON"
	::= { jdsuOnmsiPonEntry 2 }


