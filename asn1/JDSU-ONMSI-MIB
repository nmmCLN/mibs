JDSU-ONMSI-MIB DEFINITIONS ::= BEGIN

IMPORTS
	jdsuOnmsi
		FROM JDSU-SMI-MIB
	DateAndTime,
	DisplayString,
	TruthValue,
	TEXTUAL-CONVENTION
		FROM SNMPv2-TC
	IANAItuProbableCause,
	IANAItuEventType
		FROM IANA-ITU-ALARM-TC-MIB
	MODULE-IDENTITY,
	OBJECT-TYPE,
	NOTIFICATION-TYPE,
	Unsigned32
		FROM SNMPv2-SMI
	OBJECT-GROUP,
	NOTIFICATION-GROUP
		FROM SNMPv2-CONF;

-------------------------------------------------------------------------------
-- Definition of the ONMSi SNMP interface
-------------------------------------------------------------------------------

jdsuOnmsiMib MODULE-IDENTITY
	LAST-UPDATED "202003111627Z"
	ORGANIZATION "JDSU"
	CONTACT-INFO
		"JDS Uniphase France
		Postal: JDSU france
		        34 rue Necker
		        42 000 SAINT-ETIENNE
		        France

		Tel: +(33) 47747 8900
		Fax: +(33) 47747 8970"
	DESCRIPTION
		"JDSU FO Division MIB root"
	REVISION "202003111627Z"
	DESCRIPTION
		"Version 4.40: add PON scheduling and home VIP flag"
	REVISION "201911201601Z"
	DESCRIPTION
		"Version 4.30: add rootCause in PointToPointSpecificInfos"
	REVISION "201809041559Z"
	DESCRIPTION
		"Version 3.12: add alarm additional attributes"
	REVISION "201707051100Z"
	DESCRIPTION
		"Version sensing:
		Add a new value to JdsuOnmsiOtdrPosition: center(3).
		Add temperature and strain alarm specific infos to the alarm event entry and trap."
	REVISION "201210251138Z"
	DESCRIPTION
		"Version 2.40.0:
		Rename fields to attributes and custom fields to additional attributes.
		Add GPS coordinates and AlarmOriginatingEntity to the alarm event entry and trap.
		Add OTU and central office service
		Add some Textual Convention
		Update all documentation"
	REVISION "201110041115Z"
	DESCRIPTION
		"Version 2.12.1: add field tables for links, monitoring tests, PONs, homes"
	REVISION "201109271449Z"
	DESCRIPTION
		"Version 2.12.0"
	REVISION "201006090910Z"
	DESCRIPTION
		"Initial version."
	::= { jdsuOnmsi 1 }


JdsuOnmsiAlarmAckStatus ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"Enumeration to alarm acknowledgement status"
	SYNTAX INTEGER {
			acknowledged(1),
			unacknowledged(2) }


JdsuOnmsiAlarmClearStatus ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"Enumeration to alarm clear status"
	SYNTAX INTEGER {
			cleared(1),
			uncleared(2) }


JdsuOnmsiAlarmEventType ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"Enumeration for alarm event types"
	SYNTAX INTEGER {
			newAlarmEvent(1),
			changedAlarmEvent(2),
			ackStateChangedEvent(3),
			clearStateChangedEvent(4),
			alarmCommentsEvent(5) }


JdsuOnmsiInternalKey ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"Numeric object internal key"
	SYNTAX Unsigned32


JdsuOnmsiOtdrPosition ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"The position of an OTDR module in an OTU"
	SYNTAX INTEGER {
			top(1),
			bottom(2),
			center(3) }


JdsuOnmsiPeakStatus ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"Peak status :
		- UNKNOWN : before test
		- OK : peak OK
		- WARNING_LEVEL : peak crosses warning threshold in level or position
		- MINOR_LEVEL : peak crosses minor threshold in level or position
		- NEW : peak was not referenced
		- MISSING  referenced peak is missing"
	SYNTAX OCTET STRING


JdsuOnmsiPeakType ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"Enumeration for peak types"
	SYNTAX INTEGER {
			notReferenced(1),
			notHome(2),
			home(3) }


JdsuOnmsiSeverity ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"Perceived severity as defined in OSS/J"
	SYNTAX INTEGER {
			none(0),
			indeterminate(1),
			warning(2),
			minor(3),
			major(4),
			critical(5) }


JdsuOnmsiUtf8String ::= TEXTUAL-CONVENTION
	STATUS  current
	DESCRIPTION
		"UTF-8 encoded strings.

		The UTF-8 charset is specified by RFC 2279;
		the transformation format upon which it is
		based is specified in Amendment 2 of
		ISO 10646-1 and is also described in
		the Unicode Standard"
	SYNTAX OCTET STRING (SIZE (0..65535))


-------------------------------------------------------------------------------
-- Definition of the product group
-------------------------------------------------------------------------------

jdsuOnmsiProduct OBJECT IDENTIFIER 	::= { jdsuOnmsiMib 1 }

jdsuOnmsiProductDescr OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"Textual description of the product.
		It is mandatory that this only contain
		printable ASCII characters"
	::= { jdsuOnmsiProduct 1 }


jdsuOnmsiProductObjectID OBJECT-TYPE
	SYNTAX  OBJECT IDENTIFIER
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"Vendor's authoritative identification of the
		network management subsystem contained into the
		product.  This value is allocated within the SMI
		enterprises subtree (1.3.6.1.4.1) and provides an
		easy and unambiguous means for determining `what
		kind of box' is being managed"
	::= { jdsuOnmsiProduct 2 }


jdsuOnmsiProductContact OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Textual identification of the person in charge of the managed node,
		together with information on how to contact this person"
	::= { jdsuOnmsiProduct 3 }


jdsuOnmsiProductName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"Administratively-assigned name for this
		managed product"
	::= { jdsuOnmsiProduct 4 }


jdsuOnmsiProductLocation OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Physical location of the product (e.g.,
		`JDSU ONMS Server office, 3rd floor...')"
	::= { jdsuOnmsiProduct 5 }


-------------------------------------------------------------------------------
-- Definition of the administration group
-------------------------------------------------------------------------------

jdsuOnmsiAdministration OBJECT IDENTIFIER 	::= { jdsuOnmsiMib 2 }

-------------------------------------------------------------------------------
-- SNMP configuration
-------------------------------------------------------------------------------

jdsuOnmsiSnmpConfig OBJECT IDENTIFIER 	::= { jdsuOnmsiAdministration 1 }

jdsuOnmsiSnmpConfigurationReload OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Set 1 to reload the SNMP configuration.
		(from jdmk.acl and snmp.properties files)"
	::= { jdsuOnmsiSnmpConfig 1 }


-------------------------------------------------------------------------------
-- Definition of the I'm Alive feature
-------------------------------------------------------------------------------

jdsuOnmsiImAlive OBJECT IDENTIFIER 	::= { jdsuOnmsiAdministration 2 }

jdsuOnmsiImAlivePeriodMin OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The periodicity of jdsuOnmsiImAliveTrap, in minutes.
		Setting 0 will disable the traps."
	::= { jdsuOnmsiImAlive 1 }


jdsuOnmsiImAliveText OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The text carried by the jdsuOnmsiImAliveTrap"
	::= { jdsuOnmsiImAlive 2 }


jdsuOnmsiImAliveAlarmEventSequence OBJECT-TYPE
	SYNTAX  Unsigned32
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The sequence of the jdsuOnmsiAlarmEventTrap.
		Managers can rely on it to detect missed jdsuOnmsiAlarmEventTrap.
		Use jdsuOnmsiAlarmResendEvents to resend missed traps."
	::= { jdsuOnmsiImAlive 3 }


-------------------------------------------------------------------------------
-- Definition of the service group
-------------------------------------------------------------------------------

jdsuOnmsiServices OBJECT IDENTIFIER 	::= { jdsuOnmsiMib 3 }

-------------------------------------------------------------------------------
-- Definition of the home service
-------------------------------------------------------------------------------

jdsuOnmsiHomeService OBJECT IDENTIFIER 	::= { jdsuOnmsiServices 1 }

-------------------------------------------------------------------------------
-- Data of the home service
-------------------------------------------------------------------------------

jdsuOnmsiHomeData OBJECT IDENTIFIER 	::= { jdsuOnmsiHomeService 1 }

jdsuOnmsiHomeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiHomeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the standard home data"
	::= { jdsuOnmsiHomeData 1 }


jdsuOnmsiHomeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiHomeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A home"
	INDEX {
		jdsuOnmsiHomeEntryInternalKey }
	::= { jdsuOnmsiHomeTable 1 }


JdsuOnmsiHomeEntry ::= SEQUENCE {

	jdsuOnmsiHomeEntryInternalKey                    JdsuOnmsiInternalKey,
	jdsuOnmsiHomeEntryHomeIdentifier                 JdsuOnmsiUtf8String,
	jdsuOnmsiHomeEntryName                           JdsuOnmsiUtf8String,
	jdsuOnmsiHomeEntryDescription                    JdsuOnmsiUtf8String,
	jdsuOnmsiHomeEntryPonInternalKey                 JdsuOnmsiInternalKey,
	jdsuOnmsiHomeEntryHomeTerminationTypeInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiHomeEntryLatestPeakInternalKey          JdsuOnmsiInternalKey,
	jdsuOnmsiHomeEntryLatestPeakSeverity             JdsuOnmsiSeverity,
	jdsuOnmsiHomeEntryLatestPeakStatus               JdsuOnmsiPeakStatus,
	jdsuOnmsiHomeEntryVip                            TruthValue }


jdsuOnmsiHomeEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The nternal key of the home"
	::= { jdsuOnmsiHomeEntry 1 }


jdsuOnmsiHomeEntryHomeIdentifier OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The identifier of the home"
	::= { jdsuOnmsiHomeEntry 2 }


jdsuOnmsiHomeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The name of the home"
	::= { jdsuOnmsiHomeEntry 3 }


jdsuOnmsiHomeEntryDescription OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The description of the home"
	::= { jdsuOnmsiHomeEntry 4 }


jdsuOnmsiHomeEntryPonInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the home's PON"
	::= { jdsuOnmsiHomeEntry 5 }


jdsuOnmsiHomeEntryHomeTerminationTypeInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the home's termination type"
	::= { jdsuOnmsiHomeEntry 6 }


jdsuOnmsiHomeEntryLatestPeakInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the home's latest peak"
	::= { jdsuOnmsiHomeEntry 7 }


jdsuOnmsiHomeEntryLatestPeakSeverity OBJECT-TYPE
	SYNTAX  JdsuOnmsiSeverity
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The severity of the home's latest peak"
	::= { jdsuOnmsiHomeEntry 8 }


jdsuOnmsiHomeEntryLatestPeakStatus OBJECT-TYPE
	SYNTAX  JdsuOnmsiPeakStatus
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The status of the home's latest peak"
	::= { jdsuOnmsiHomeEntry 9 }


jdsuOnmsiHomeEntryVip OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The VIP status of the home"
	::= { jdsuOnmsiHomeEntry 10 }


jdsuOnmsiHomeAdditionalAttributeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiHomeAdditionalAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the additional attributes on homes"
	::= { jdsuOnmsiHomeData 2 }


jdsuOnmsiHomeAdditionalAttributeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiHomeAdditionalAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A additional attribute on a home"
	INDEX {
		jdsuOnmsiHomeAdditionalAttributeEntryInternalKey,
		jdsuOnmsiHomeAdditionalAttributeEntryName }
	::= { jdsuOnmsiHomeAdditionalAttributeTable 1 }


JdsuOnmsiHomeAdditionalAttributeEntry ::= SEQUENCE {

	jdsuOnmsiHomeAdditionalAttributeEntryInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiHomeAdditionalAttributeEntryName        JdsuOnmsiUtf8String,
	jdsuOnmsiHomeAdditionalAttributeEntryValue       JdsuOnmsiUtf8String }


jdsuOnmsiHomeAdditionalAttributeEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the home"
	::= { jdsuOnmsiHomeAdditionalAttributeEntry 1 }


jdsuOnmsiHomeAdditionalAttributeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute name"
	::= { jdsuOnmsiHomeAdditionalAttributeEntry 2 }


jdsuOnmsiHomeAdditionalAttributeEntryValue OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The additional attribute value"
	::= { jdsuOnmsiHomeAdditionalAttributeEntry 3 }


jdsuOnmsiHomeTerminationTypeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiHomeTerminationTypeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the standard home termination type data"
	::= { jdsuOnmsiHomeData 3 }


jdsuOnmsiHomeTerminationTypeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiHomeTerminationTypeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A home termination type"
	INDEX {
		jdsuOnmsiHomeTerminationTypeEntryInternalKey }
	::= { jdsuOnmsiHomeTerminationTypeTable 1 }


JdsuOnmsiHomeTerminationTypeEntry ::= SEQUENCE {

	jdsuOnmsiHomeTerminationTypeEntryInternalKey JdsuOnmsiInternalKey,
	jdsuOnmsiHomeTerminationTypeEntryName        JdsuOnmsiUtf8String,
	jdsuOnmsiHomeTerminationTypeEntryDescription JdsuOnmsiUtf8String }


jdsuOnmsiHomeTerminationTypeEntryInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The internal key of the home termination type"
	::= { jdsuOnmsiHomeTerminationTypeEntry 1 }


jdsuOnmsiHomeTerminationTypeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The name of the home termination type"
	::= { jdsuOnmsiHomeTerminationTypeEntry 2 }


jdsuOnmsiHomeTerminationTypeEntryDescription OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The description of the home termination type"
	::= { jdsuOnmsiHomeTerminationTypeEntry 3 }


jdsuOnmsiHomeAttributeTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF JdsuOnmsiHomeAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"This table contains the descriptions of noticeable home attributes"
	::= { jdsuOnmsiHomeData 4 }


jdsuOnmsiHomeAttributeEntry OBJECT-TYPE
	SYNTAX  JdsuOnmsiHomeAttributeEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"A home attribute entry"
	INDEX {
		jdsuOnmsiHomeAttributeEntryName }
	::= { jdsuOnmsiHomeAttributeTable 1 }


JdsuOnmsiHomeAttributeEntry ::= SEQUENCE {

	jdsuOnmsiHomeAttributeEntryName       JdsuOnmsiUtf8String,
	jdsuOnmsiHomeAttributeEntryAdditional TruthValue,
	jdsuOnmsiHomeAttributeEntryFindable   TruthValue,
	jdsuOnmsiHomeAttributeEntryUpdatable  TruthValue }


jdsuOnmsiHomeAttributeEntryName OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The attribute name. The additional attributes are prefixed with an underscore character to avoid name collision with standard attributes"
	::= { jdsuOnmsiHomeAttributeEntry 1 }


jdsuOnmsiHomeAttributeEntryAdditional OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is additional"
	::= { jdsuOnmsiHomeAttributeEntry 2 }


jdsuOnmsiHomeAttributeEntryFindable OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is findable"
	::= { jdsuOnmsiHomeAttributeEntry 3 }


jdsuOnmsiHomeAttributeEntryUpdatable OBJECT-TYPE
	SYNTAX  TruthValue
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This flag indicates whether the attribute is updatable"
	::= { jdsuOnmsiHomeAttributeEntry 4 }


-------------------------------------------------------------------------------
-- Functions of the home service
-------------------------------------------------------------------------------

jdsuOnmsiHomeFunctions OBJECT IDENTIFIER 	::= { jdsuOnmsiHomeService 2 }

-------------------------------------------------------------------------------
-- Function to load the home termination types
-- Results will fill the jdsuOnmsiHomeTerminationTypeTable
-------------------------------------------------------------------------------

jdsuOnmsiHomeTerminationTypeLoad OBJECT IDENTIFIER 	::= { jdsuOnmsiHomeFunctions 1 }

jdsuOnmsiHomeTerminationTypeLoadExecute OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Setting this field to 1 performs the jdsuOnmsiHomeTerminationTypeTable load"
	::= { jdsuOnmsiHomeTerminationTypeLoad 1 }


jdsuOnmsiHomeTerminationTypeLoadError OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"When the function execution fails, this node contains information on the error that occurred.
		This node is empty when the execution is successful."
	::= { jdsuOnmsiHomeTerminationTypeLoad 2 }


-------------------------------------------------------------------------------
-- Function to get a home
-- Results will fill jdsuOnmsiHomeTable and jdsuOnmsiHomeAdditionalAttributeTable
-------------------------------------------------------------------------------

jdsuOnmsiHomeGet OBJECT IDENTIFIER 	::= { jdsuOnmsiHomeFunctions 2 }

jdsuOnmsiHomeGetParamInternalKey OBJECT-TYPE
	SYNTAX  JdsuOnmsiInternalKey
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The internal key of the home to retrieve"
	::= { jdsuOnmsiHomeGet 1 }


jdsuOnmsiHomeGetExecute OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Setting this field to 1 performs the home retrieval"
	::= { jdsuOnmsiHomeGet 2 }


jdsuOnmsiHomeGetError OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"When the function execution fails, this node contains information on the error that occurred.
		This node is empty when the execution is successful."
	::= { jdsuOnmsiHomeGet 3 }


-------------------------------------------------------------------------------
-- Function to find homes
-- Results will fill jdsuOnmsiHomeTable and jdsuOnmsiHomeAdditionalAttributeTable
-------------------------------------------------------------------------------

jdsuOnmsiHomeFind OBJECT IDENTIFIER 	::= { jdsuOnmsiHomeFunctions 3 }

jdsuOnmsiHomeFindParamAttribute OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The attribute of the home to perform the search on.
		Valid attributes for homes are :
		- name
		- identifier
		- ponInternalKey
		and any additional attribute name"
	::= { jdsuOnmsiHomeFind 1 }


jdsuOnmsiHomeFindParamValue OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"The attribute value to find"
	::= { jdsuOnmsiHomeFind 2 }


jdsuOnmsiHomeFindExecute OBJECT-TYPE
	SYNTAX  INTEGER
	MAX-ACCESS read-write
	STATUS  current
	DESCRIPTION
		"Setting this field to 1 performs the home find"
	::= { jdsuOnmsiHomeFind 3 }


jdsuOnmsiHomeFindError OBJECT-TYPE
	SYNTAX  JdsuOnmsiUtf8String
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"When the function execution fails, this node contains information on the error that occurred.
		This node is empty when the execution is successful."
	::= { jdsuOnmsiHomeFind 4 }


