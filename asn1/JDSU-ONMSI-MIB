-------------------------------------------------------------------------------
-- Definition of the event group
-------------------------------------------------------------------------------

jdsuOnmsiEvents OBJECT IDENTIFIER 	::= { jdsuOnmsiMib 4 }


jdsuOnmsiImAliveTrap NOTIFICATION-TYPE
	OBJECTS {
		jdsuOnmsiImAliveText,
		jdsuOnmsiImAliveAlarmEventSequence}
	STATUS  current
	DESCRIPTION
		"This trap is send periodically, to ensure the managers that the application is running.
		Period and text can be configured under jdsuOnmsiAdministration/jdsuOnmsiImAlive.
		This trap contains the current alarm event sequence, to help managers in detecting missed jdsuOnmsiAlarmEventTrap"
	::= { jdsuOnmsiEvents 1 }


jdsuOnmsiHomeTestResultTrap NOTIFICATION-TYPE
	OBJECTS {
		jdsuOnmsiHomeStartTestError,
		jdsuOnmsiHomeEntryInternalKey,
		jdsuOnmsiHomeEntryHomeIdentifier,
		jdsuOnmsiHomeEntryName,
		jdsuOnmsiHomeEntryDescription,
		jdsuOnmsiHomeEntryPonInternalKey,
		jdsuOnmsiHomeEntryHomeTerminationTypeInternalKey,
		jdsuOnmsiHomeEntryLatestPeakInternalKey,
		jdsuOnmsiHomeEntryLatestPeakSeverity,
		jdsuOnmsiHomeEntryLatestPeakStatus}
	STATUS  current
	DESCRIPTION
		"This trap is send after a home test is started with jdsuOnmsiHomeStartTest function."
	::= { jdsuOnmsiEvents 2 }


jdsuOnmsiPonTestResultTrap NOTIFICATION-TYPE
	OBJECTS {
		jdsuOnmsiPonStartTestError,
		jdsuOnmsiPonEntryInternalKey,
		jdsuOnmsiPonEntryName,
		jdsuOnmsiPonEntryDescription,
		jdsuOnmsiPonEntryLatestTestInternalKey,
		jdsuOnmsiPonEntryOtuInternalKey,
		jdsuOnmsiPonEntryPortInternalKey,
		jdsuOnmsiPonEntrySeverity}
	STATUS  current
	DESCRIPTION
		"This trap is send after a PON test is started with jdsuOnmsiPonStartTest function."
	::= { jdsuOnmsiEvents 3 }


jdsuOnmsiMonitoringTestResultTrap NOTIFICATION-TYPE
	OBJECTS {
		jdsuOnmsiMonitoringTestStartTestError,
		jdsuOnmsiMonitoringTestEntryInternalKey}
	STATUS  current
	DESCRIPTION
		"This trap is send after a monitoring test is started with jdsuOnmsiMonitoringTestStartTest function."
	::= { jdsuOnmsiEvents 4 }


jdsuOnmsiAlarmEventTrap NOTIFICATION-TYPE
	OBJECTS {
		jdsuOnmsiAlarmEventEntrySequence,
		jdsuOnmsiAlarmEventEntryEventInternalKey,
		jdsuOnmsiAlarmEventEntryEventType,
		jdsuOnmsiAlarmEventEntryEventTime,
