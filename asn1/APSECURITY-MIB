-- GTP Error Stats 
    apSecurityGtpErrorStatsTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF ApSecurityGtpErrorStatsEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "The table of Gtp error stats per Gtp-profile for Gtp negotiations."
        ::= { apSecurityMIBObjects 13 }

    apSecurityGtpErrorStatsEntry OBJECT-TYPE
        SYNTAX         ApSecurityGtpErrorStatsEntry
        MAX-ACCESS     not-accessible
        STATUS         current
        DESCRIPTION    "Gtp negotiation error statistics for  GTP-Profile."
        AUGMENTS	{ apSecurityGtpStatsEntry }
        ::= { apSecurityGtpErrorStatsTable 1 }

    ApSecurityGtpErrorStatsEntry ::=
        SEQUENCE {
                apSecurityGtpMsgDiffVer				Counter32,
                apSecurityGtpInvalidMsgLen			Counter32,
                apSecurityGtpUnknownMsg				Counter32,
                apSecurityGtpUnexpectedMsg			Counter32,
                apSecurityGtpMissingMandIE			Counter32,
                apSecurityGtpMissingReqIE			Counter32,
                apSecurityGtpMissingContIE			Counter32,
                apSecurityGtpInvalidIELen			Counter32,
                apSecurityGtpInvalidMandIE			Counter32,
                apSecurityGtpInvalidOptIE			Counter32,
                apSecurityGtpUnknownIE				Counter32,
                apSecurityGtpInvalidSeqIE			Counter32,
                apSecurityGtpUnexpectedIE			Counter32,
                apSecurityGtpRepeatedIE				Counter32,
                apSecurityGtpIncorrectOptIE		   	Counter32,
                apSecurityGtpTunnelNonExistent			Counter32,
                apSecurityGtpInvalidMsgFormat			Counter32,
                apSecurityGtpErrorIndCtxtNotFound		Counter32,
		        apSecurityGtpInvalidPCOFormat			Counter32,
                apSecurityGtpSemanticTFTError			Counter32,
                apSecurityGtpSyntacticTFTError			Counter32,
                apSecurityGtpSemanticPFError			Counter32,
                apSecurityGtpSyntacticPFError			Counter32,
		        apSecurityGtpInvalidAPCOFormat			Counter32
        }
    
    apSecurityGtpMsgDiffVer OBJECT-TYPE
        SYNTAX          Counter32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "The count of  messages with different Gtp version on Gtp profile."
