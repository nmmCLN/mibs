-- TSCF ADDRESS POOL STATISTICS

apSecurityTscfAddressPoolStatsTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF ApSecurityTscfAddressPoolStatsEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "The table of TSCF address pool statistics."
        ::= { apSecurityMIBObjects 15 }


apSecurityTscfAddressPoolStatsEntry OBJECT-TYPE
        SYNTAX          ApSecurityTscfAddressPoolStatsEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "TSCF Address pool statistics."
        INDEX   { apSecurityTscfAddressPoolName }
        ::= { apSecurityTscfAddressPoolStatsTable 1 }

ApSecurityTscfAddressPoolStatsEntry ::=
        SEQUENCE {
                apSecurityTscfAddressPoolName                    DisplayString,
                apSecurityTscfTotalAddressCount                  Gauge32,
                apSecurityTscfInUseAddressCount                  Gauge32,
                apSecurityTscfFreeAddressCount                   Gauge32,
                apSecurityTscfInvalidAddressCount                Gauge32
                }

apSecurityTscfAddressPoolName                    OBJECT-TYPE
        SYNTAX                  DisplayString (SIZE (0..255))
        MAX-ACCESS              accessible-for-notify
        STATUS                  current
        DESCRIPTION
                "The name of the TSCF Address Pool"
        ::= { apSecurityTscfAddressPoolStatsEntry 1}

apSecurityTscfTotalAddressCount     OBJECT-TYPE
        SYNTAX               Gauge32
        MAX-ACCESS           accessible-for-notify
        STATUS               current
        DESCRIPTION
                "The total numbers of addresses in the TSCF address pool"
        ::= { apSecurityTscfAddressPoolStatsEntry 2 }

apSecurityTscfInUseAddressCount                  OBJECT-TYPE
        SYNTAX               Gauge32
        MAX-ACCESS           accessible-for-notify
        STATUS               current
        DESCRIPTION
                "The number of addresses in use from the TSCF address pool"
        ::= { apSecurityTscfAddressPoolStatsEntry 3 }

apSecurityTscfFreeAddressCount                     OBJECT-TYPE
        SYNTAX               Gauge32
        MAX-ACCESS           accessible-for-notify
        STATUS               current
        DESCRIPTION
                "The number of addresses available for allocation in the TSCF address pool"
        ::= { apSecurityTscfAddressPoolStatsEntry 4 }

apSecurityTscfInvalidAddressCount                 OBJECT-TYPE
        SYNTAX               Gauge32
        MAX-ACCESS           accessible-for-notify
        STATUS               current
        DESCRIPTION
                "The number of invalid  addresses in the TSCF address pool"
        ::= { apSecurityTscfAddressPoolStatsEntry 5 }


-- TSCF notifications

    apSecurityTscfAddrPoolThresholdLevel     OBJECT-TYPE
        SYNTAX          Gauge32
        MAX-ACCESS      accessible-for-notify
        STATUS          current
        DESCRIPTION
            "TSCF address pool threshold"
        ::= { apSecurityNotificationObjects 29 }

    apSecurityTscfMaxTunnelsThresholdLevel   OBJECT-TYPE
        SYNTAX          Gauge32
        MAX-ACCESS      accessible-for-notify
        STATUS          current
        DESCRIPTION
            "TSCF max tunnels threshold"
        ::= { apSecurityNotificationObjects 30 }

    apSecurityTscfNotificationsPrefix
                OBJECT IDENTIFIER ::= { apSecurityNotifications 8 }

    apSecurityTscfNotifications
                OBJECT IDENTIFIER ::= { apSecurityTscfNotificationsPrefix 0 }


    apSecurityTscfAddressPoolUsageTrap   NOTIFICATION-TYPE
        OBJECTS
        {
                        apSecurityTscfAddressPoolName, 
                        apSecurityTscfInUseAddressCount,
                        apSecurityTscfTotalAddressCount,
                        apSecurityTscfAddrPoolThresholdLevel
        }
        STATUS          current
        DESCRIPTION
                        "This notification is sent when the available IP addresses
                        in TSCF address pool depletes below the 
                        threshold level."
                
        ::= { apSecurityTscfNotifications 1 }


    apSecurityTscfAddressPoolUsageClearTrap  NOTIFICATION-TYPE
        OBJECTS
        {
                        apSecurityTscfAddressPoolName, 
                        apSecurityTscfInUseAddressCount,
                        apSecurityTscfTotalAddressCount,
                        apSecurityTscfAddrPoolThresholdLevel
        }
        STATUS          current
        DESCRIPTION
                        "This notification is sent when the available 
                        IP addresses in TSCF address pool 
                        are more than the threshold level."
                
        ::= { apSecurityTscfNotifications 2 }

    apSecurityTscfMaxTunnelTrap  NOTIFICATION-TYPE
        OBJECTS
        {
                        apTscfActiveTunnels,
                        apTscfLicenseTunnelCount,
                        apSecurityTscfMaxTunnelsThresholdLevel
        }
        STATUS          current
        DESCRIPTION
                        "This notification is sent when the available
                        IP addresses in TSCF address pool 
                        depletes below the threshold level."
                
        ::= { apSecurityTscfNotifications 3 }

    apSecurityTscfClearMaxTunnelTrap  NOTIFICATION-TYPE
        OBJECTS
        {
                        apTscfActiveTunnels,
                        apTscfLicenseTunnelCount,
                        apSecurityTscfMaxTunnelsThresholdLevel
        }
        STATUS          current
        DESCRIPTION
                        "This notification is sent when the available
                        IP addresses in TSCF address pool 
                        depletes below the threshold level."
                
        ::= { apSecurityTscfNotifications 4 }

    apSecurityTscfNotificationsGroup  NOTIFICATION-GROUP
        NOTIFICATIONS
        {
                apSecurityTscfAddressPoolUsageTrap,
                apSecurityTscfAddressPoolUsageClearTrap,
                apSecurityTscfMaxTunnelTrap,
                apSecurityTscfClearMaxTunnelTrap
                }
        STATUS                           current
        DESCRIPTION
                 "A collection of notifications for TSCF resource alerts"
        ::= {  apSecurityNotificationsGroups 11 }

END
