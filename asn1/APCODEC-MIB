apCodecTranscodingDSPMIPSProtection OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "Indication whether DSP MIPS protection is active or not"
        ::= { apCodecTranscodingResourceUtilMIBObjects 9 }

-- TCM Load Statistics
apCodecTranscodingTCULoadStatsTable OBJECT-TYPE
        SYNTAX                  SEQUENCE OF ApCodecTranscodingTCULoadStatsEntry
        MAX-ACCESS              not-accessible
        STATUS                  current
        DESCRIPTION
            "Transcoding module (TCM) load both in number of 
             sessions and percent loading"
        ::= { apCodecTranscodingMIBObjects 6 }

apCodecTranscodingTCULoadStatsEntry  OBJECT-TYPE
        SYNTAX                  ApCodecTranscodingTCULoadStatsEntry
        MAX-ACCESS              not-accessible
        STATUS                  current
        DESCRIPTION
            "A table entry designed to hold Transcoding module (TCM) load"
        INDEX { apCodecTranscodingTCUIndex, apCodecTranscodingTCMIndex, apCodecTranscodingDSPIndex }
        ::= { apCodecTranscodingTCULoadStatsTable 1 }

ApCodecTranscodingTCULoadStatsEntry ::= SEQUENCE {
        apCodecTranscodingTCUIndex              Integer32,
        apCodecTranscodingTCMIndex              Integer32,
        apCodecTranscodingDSPIndex              Integer32,
        apCodecTranscodingSessionCount          Gauge32,
        apCodecTranscodingCurrentLoad           Gauge32,
        apCodecTranscodingPeakLoad              Gauge32,
        apCodecTranscodingDSPOverloaded         TruthValue
}

apCodecTranscodingTCUIndex OBJECT-TYPE
        SYNTAX                  Integer32 (0..2147483647)
        MAX-ACCESS              not-accessible
        STATUS                  current
        DESCRIPTION
            "An integer for the sole purpose of indexing the TCU in a system. 
             This index is persistent across reboots and therefore 
             may not necessarily be continuous"
        ::= {apCodecTranscodingTCULoadStatsEntry 1}

apCodecTranscodingTCMIndex OBJECT-TYPE
        SYNTAX                  Integer32 (0..2147483647)
        MAX-ACCESS              not-accessible
        STATUS                  current
        DESCRIPTION
            "An integer for the sole purpose of indexing the TCM within a TCU. 
             This index is persistent across reboots and therefore 
             may not necessarily be continuous"
        ::= { apCodecTranscodingTCULoadStatsEntry 2}

apCodecTranscodingDSPIndex OBJECT-TYPE
        SYNTAX                  Integer32 (0..2147483647)
        MAX-ACCESS              not-accessible
        STATUS                  current
        DESCRIPTION
            "An integer for the sole purpose of indexing a DSP within a TCM. 
             This index is persistent across reboots and therefore 
             may not necessarily be continuous"
        ::= { apCodecTranscodingTCULoadStatsEntry 3}

apCodecTranscodingSessionCount OBJECT-TYPE
        SYNTAX                  Gauge32
        MAX-ACCESS              read-only
        STATUS                  current
        DESCRIPTION
            "Total number of sessions for a DSP"
        ::= { apCodecTranscodingTCULoadStatsEntry 4}

apCodecTranscodingCurrentLoad OBJECT-TYPE
        SYNTAX                  Gauge32
        UNITS                   "one hundredth"
        MAX-ACCESS              read-only
        STATUS                  current
        DESCRIPTION
            "Current load in percentage for a DSP"
        ::= { apCodecTranscodingTCULoadStatsEntry 5}
    
apCodecTranscodingPeakLoad OBJECT-TYPE
        SYNTAX                  Gauge32
        UNITS                   "one hundredth"
        MAX-ACCESS              read-only
        STATUS                  current
        DESCRIPTION
            "Peak load in percentage for a DSP"
        ::= { apCodecTranscodingTCULoadStatsEntry 6}

apCodecTranscodingDSPOverloaded OBJECT-TYPE
        SYNTAX                  TruthValue
        MAX-ACCESS              read-only
        STATUS                  current
        DESCRIPTION
            "Indication whether the DSP is overloaded or not"
        ::= { apCodecTranscodingTCULoadStatsEntry 7}   

-- Notification MIBs

