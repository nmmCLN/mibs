	SYNTAX 			DisplayString (SIZE (0..255))
	MAX-ACCESS		accessible-for-notify
	STATUS			current
	DESCRIPTION
	    "From header value for Recording Dialog"
	::= { apSipRecNotifyObjects 3 }

