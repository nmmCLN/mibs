APSIP-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        Gauge32, Integer32, Unsigned32, Counter32, Counter64
                FROM SNMPv2-SMI
        TEXTUAL-CONVENTION, DisplayString, TruthValue
                FROM SNMPv2-TC
        MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
                FROM SNMPv2-CONF
        InetAddress, InetAddressType
            FROM INET-ADDRESS-MIB
        acmepacketMgmt
                FROM ACMEPACKET-SMI
        ApHardwareModuleFamily, ApRedundancyState, ApPhyPortType,
        ApPresence, ApServerStatus, ApSipMethod
            FROM ACMEPACKET-TC
        apSysMgmtSipInterfaceRealmName, apSysMgmtSipInterfaceIP,
        apSipSessionAgentStatsEntry, apSigRealmStatsEntry
                 FROM APSYSMGMT-MIB;

apSipModule   MODULE-IDENTITY
		LAST-UPDATED	"201904240000Z"
	    	ORGANIZATION 	"Oracle Communications"
	    	CONTACT-INFO
			"           	Customer Service
		 	Postal:		Oracle Communications
					100 Crosby Drive 
					Bedford, MA 01730
					US
		    	Tel:		1-800-633-0738
			Url:		www.oracle.com
		 	E-mail:		support@oracle.com"
                DESCRIPTION
                        "SIP MIB for Oracle Communications Acme Packet SBCs"
                REVISION        "201207130000Z"
                DESCRIPTION
                        "Updated contact info."
                REVISION        "201203070000Z"
                DESCRIPTION
                        "Initial version."
		REVISION 	"201406260000Z" 			
	    	DESCRIPTION
		    	"Updated Organization and Contact info."
                REVISION    "201711240000Z"
                DESCRIPTION
                        "Added MIB definitions for SRVCC Stats."
                REVISION 	"201712130000Z" 			
	        DESCRIPTION
		   	"Added new table for sip method stats (apSipMethodStatsTable)"
                REVISION    "201804040000Z"
                DESCRIPTION
                        "Added stats for SRVCC pre-alerting."
                REVISION 	"201804270000Z" 			
	        DESCRIPTION
		   	"Added MIB definitions for SIP Reg Event Subscription Stats."
                REVISION    "201904240000Z"
                DESCRIPTION
                        "Added MIB definitions for MSRP Stats."

                ::= { acmepacketMgmt 15 }


