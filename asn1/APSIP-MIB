apSipCACNotificationsGroup  NOTIFICATION-GROUP
      NOTIFICATIONS {
		apSipCACUtilAlertTrap,
      	apSipCACUtilClearTrap
 	}
	STATUS          current
	DESCRIPTION
		"Traps to monitor SIP CAC utilization."
	::= {  apSipNotificationGroups 3 }

-- siprec
apSipRecModule			OBJECT IDENTIFIER ::= { apSipModule 4 }
apSipRecNotifications		OBJECT IDENTIFIER ::= { apSipRecModule 1 }
apSipRecNotificationPrefix	OBJECT IDENTIFIER ::= { apSipRecNotifications 0 }
apSipRecNotifyObjects		OBJECT IDENTIFIER ::= { apSipRecNotifications 1 }

-- siprec notification objects
apSipRecRecDlgCallIdHeader	OBJECT-TYPE
	SYNTAX 			DisplayString (SIZE (0..255))
	MAX-ACCESS		accessible-for-notify
	STATUS			current
	DESCRIPTION
	    "Call-ID header value for Recording Dialog"
	::= { apSipRecNotifyObjects 1 }

apSipRecRecDlgToHeader		OBJECT-TYPE
	SYNTAX 			DisplayString (SIZE (0..255))
	MAX-ACCESS		accessible-for-notify
	STATUS			current
	DESCRIPTION
	    "To header value for Recording Dialog"
	::= { apSipRecNotifyObjects 2 }

